{
    "name":"psecio/parse",
    "type":"library",
    "description":"A PHP Security Scanner",
    "keywords":["security","scanner","static"],
    "homepage":"https://github.com/lachee/parse.git",
    "license":"MIT",
    "authors":[
        {
            "name":"Chris Cornutt",
            "email":"ccornutt@phpdeveloper.org",
            "homepage":"http://www.phpdeveloper.org/"
        },
        {
            "name":"Lachee",
            "homepage": "https://lachee.dev"
        }
    ],
    "require":{
        "php": ">=5.4",
        "nikic/php-parser": "^5.0",
        "symfony/console": "2.5 - 3.2",
        "symfony/event-dispatcher": "2.4 - 3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Psecio\\Parse\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Psecio\\Parse\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "bin": [
        "bin/psecio-parse",
        "bin/parse"
    ]
}
